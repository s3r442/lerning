x = 5
y = 10
x = x+2 
x+=2
x = x*2 
x*=2
x = x/2 
x/=2
x = x-2 
x-=2
print(x)
#операторы присваивания
print(x-y)
print(x+y)
print(x/y)
print(x*y)
#арифметические операторы
x<y
x>y
x==y
x<=y
x>=y
x!=y
#операторы сравнения
x<8 and y>11
x<8 or y>11
x = True
x = not x
#логические операторы
a,b=1,4
a = -a
print(a)
#унарный оператор
